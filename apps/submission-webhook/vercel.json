{
    "version":2,
    "builds":[{
        "src":"src/index.ts",
        "use":"@vercel/node",
        "config": {
            "includeFiles": ["apps/submission-webhook/src/**", "apps/submission-webhook/tsconfig.json", "apps/submission-webhook/package.json"]
          }
